{
    "presets": ["@babel/preset-env"],
    "plugins": [
        [
            "module-resolver",
            {
                "alias": {
                    "~": "./src"
                }
            }
        ],
        [
            "@babel/plugin-syntax-import-attributes",
            {
                "deprecatedAssertSyntax": true
            }
        ],
        ["@babel/transform-runtime"],
        ["@babel/plugin-transform-modules-commonjs"]
    ]
}
